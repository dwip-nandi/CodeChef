#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin>>n;
    vector<int>v(n);
    int ec=0,oc=0;
    for(int i=0;i<n;i++){
        cin>>v[i];
        if(v[i]%2==0)ec++;
        else oc++;
    }
    if(ec>oc)cout<<"READY FOR BATTLE"<<endl;
    else  cout<<"NOT READY" <<endl;

}
